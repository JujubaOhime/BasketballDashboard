<table class="table table-hover">
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cidade</th>
      <th>Imagem</th>
      <th>Bras√£o do Time</th>
      <th colspan="1">Ver</th>
    </tr>
  </thead>

  <tbody>
    <% @players.each do |player| %>
      <tr>
        <td><%= player.first_name %></td>
        <td><%= player.birth_city %></td>
        <td> <%= image_tag player.photo_url, size: "50x50" %> </td>
        <td> <%= image_tag player.team.wikipedia_logo_url, size: "100x100" %> </td>
        <td><%= link_to "ver", player_path(player) %></td>
      </tr>
    <% end %>
  </tbody>
</table>